{
  "name": "town-media-agent-full-stack",
  "version": "1.0.0",
  "description": "Town Media Agent - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "npm run dev",
    "setup": "npm run install-all"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "town-media",
    "social-media",
    "agency",
    "fullstack",
    "nextjs",
    "nodejs"
  ],
  "author": "Town Media Team",
  "license": "MIT"
}
